        </Label>
        <Label x:Name="KDLabelN" Content="1.23" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontFamily="./Resources/#SplatFont 2" FontSize="50" Margin="0,103,43,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
    </Grid>
</Window>
