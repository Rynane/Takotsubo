        <Label Content="d" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="#9A9A9A" FontFamily="./Resources/#SplatFont 2" FontSize="25" Margin="0,13,382,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label x:Name="KALabel" Content="200(100)" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="#9A9A9A" FontFamily="./Resources/#SplatFont 2" FontSize="35" Margin="0,5,476,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label x:Name="DLabel" Content="100" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="#9A9A9A" FontFamily="./Resources/#SplatFont 2" FontSize="35" Margin="0,5,396,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label x:Name="KDLabel" Content="1.23" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="#9A9A9A" FontFamily="./Resources/#SplatFont 2" FontSize="35" Margin="0,5,312,0">
