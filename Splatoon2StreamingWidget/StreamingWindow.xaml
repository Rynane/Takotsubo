<Window x:Class="Splatoon2StreamingWidget.StreamingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Splatoon2StreamingWidget"
        mc:Ignorable="d"
        Title="StreamingWindow" Height="210" Width="800" Foreground="{x:Null}" Background="{x:Null}" ResizeMode="NoResize">
    <Grid>
        <Rectangle  Fill="#3A3A3A" Stroke="#FF000000" RadiusX="15" RadiusY="15"/>
        <Label x:Name="UdemaeLabel" Content="X" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontFamily="./Resources/#Splatfont 2" FontSize="150" Margin="15,20,0,0" FontWeight="Normal" FontStyle="Normal" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="1.3"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label x:Name="XPowerLabel" Content="2000.0" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontFamily="./Resources/#Splatfont 2" FontSize="130" Margin="90,35,0,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label x:Name="XPowerSubtractLabel" Content="-10.0" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="Red" FontFamily="./Resources/#Splatfont 2" FontSize="65" Margin="370,91,0,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label x:Name="RuleLabel" Content="ガチエリア" HorizontalAlignment="Left" VerticalAlignment="Top" Background="{x:Null}" Foreground="#E2562C" FontFamily="./Resources/#イカモドキ" FontSize="35" Margin="10,-5,0,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="1" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label Content="k/d" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="#9A9A9A" FontFamily="./Resources/#SplatFont 2" FontSize="25" Margin="0,13,280,0">
            <Label.Effect>
                <DropShadowEffect ShadowDepth="4" Direction="330" Color="Black" Opacity="0.5" BlurRadius="4"/>
            </Label.Effect>
        </Label>
        <Label Content="k" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="#9A9A9A" FontFamily="./Resources/#SplatFont 2" FontSize="25" Margin="0,13,461,0">
